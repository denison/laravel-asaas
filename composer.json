{
    "name": "denison/asaas-package",
    "description": "Pacote simples de pagamento para Laravel usando o Asaas.",
    "type": "library",
    "require": {
      "php": "^7.4 || ^8.0",
      "illuminate/support": "^8.0 || ^9.0 || ^10.0",
      "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
      "psr-4": {
        "Denison\\AsaasPackage\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Denison\\AsaasPackage\\AsaasServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.5 || ^11.4",
        "vlucas/phpdotenv": "^5.4"
    }
}
